<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    Deferred Results Option Adapter WSDL
  -->
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" 
        xmlns="http://schemas.xmlsoap.org/wsdl/" 
        xmlns:tns="urn:gov:hhs:fha:nhinc:dq:adapterdeferredresultoption"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
        xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
        xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl"
        xmlns:rs="urn:oasis:names:tc:ebxml-regrep:xsd:rs:3.0"
        xmlns:nccommonadapter="urn:gov:hhs:fha:nhinc:common:nhinccommonadapter"
        name="AdapterDocQueryDeferredResultsOption" 
        targetNamespace="urn:gov:hhs:fha:nhinc:dq:adapterdeferredresultoption">
    <documentation>Deferred Results Option Adapter</documentation>
    <types>
        <xsd:schema>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonadapter" schemaLocation="classpath:schemas/nhinc/common/NhincCommonAdapter.xsd"/>
            <xsd:import namespace="urn:oasis:names:tc:ebxml-regrep:xsd:rs:3.0" schemaLocation="classpath:schemas/ebRS/rs.xsd"/>
        </xsd:schema>
    </types>   
    <message name="RespondingGateway_CrossGatewayQueryDeferredResultsMessage">
        <part name="RespondingGateway_CrossGatewayQueryDeferredResults" element="nccommonadapter:RespondingGateway_CrossGatewayQueryResponse"/>
    </message>
    <message name="RespondingGateway_CrossGatewayQueryDeferredResponseMessage">
        <part name="RespondingGateway_CrossGatewayQueryDeferredResponse" element="rs:RegistryResponse"/>
    </message>
     <portType name="AdapterDocQueryDeferredResultsOptionPortType">
        <operation name="RespondingGateway_CrossGatewayQueryDeferredResults">
            <input name="RespondingGateway_CrossGatewayQueryDeferredResults" message="tns:RespondingGateway_CrossGatewayQueryDeferredResultsMessage" wsaw:Action="urn:gov:hhs:fha:nhinc:dq:adapterdeferredresultoption:RespondingGateway_CrossGatewayQueryDeferredResults" />
            <output name="RespondingGateway_CrossGatewayQueryDeferredResponse" message="tns:RespondingGateway_CrossGatewayQueryDeferredResponseMessage" wsaw:Action="urn:gov:hhs:fha:nhinc:dq:adapterdeferredresultoption:RespondingGateway_CrossGatewayQueryDeferredResponse"/>
        </operation>
    </portType>
    <binding name="AdapterDeferredResultsOptionBindingSoap" type="tns:AdapterDocQueryDeferredResultsOptionPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="RespondingGateway_CrossGatewayQueryDeferredResults">
            <soap:operation soapAction="urn:gov:hhs:fha:nhinc:dq:adapterdeferredresultoption:RespondingGateway_CrossGatewayQueryDeferredResults"/>
            <input name="RespondingGateway_CrossGatewayQueryDeferredResults">
                <soap:body use="literal"/>
            </input>
            <output name="RespondingGateway_CrossGatewayQueryDeferredResponse">
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>
    <service name="AdapterDeferredResultsOption">
        <port name="AdapterDeferredResultsOptionPortSoap"  binding="tns:AdapterDeferredResultsOptionBindingSoap">
            <soap:address location="http://localhost:${HttpDefaultPort}/NhinConnect/AdapterDeferredResultsOption" />
        </port>
    </service>
</definitions>